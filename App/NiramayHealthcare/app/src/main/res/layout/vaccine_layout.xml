<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#00ffffff"
    android:orientation="vertical">


    <View
        android:layout_width="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_height="2dp"
        android:layout_marginTop="5dp"
        android:background="@color/black"/>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:rowCount="1"
        android:columnCount="7"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:layout_columnWeight="2">
            <TextView
                android:id="@+id/center_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="NA"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15dp" />
        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_gravity="center_vertical"
            android:layout_height="50dp"
            android:background="@color/black"/>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_gravity="center_vertical"
            android:layout_columnWeight="1">
            <TextView
                android:id="@+id/age_Textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="18+"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15dp" />
        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_gravity="center_vertical"
            android:layout_height="50dp"
            android:background="@color/black"/>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_columnWeight="1"
            android:gravity="center">

            <TextView
                android:id="@+id/dose1_Textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="NA"
                android:textColor="@color/black"
                android:textSize="15dp" />
        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_gravity="center_vertical"
            android:layout_height="50dp"
            android:background="@color/black"/>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:layout_columnWeight="1">
            <TextView
                android:id="@+id/dose2_Textview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NA"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15dp" />
        </LinearLayout>


    </GridLayout>
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:rowCount="1"
        android:columnCount="7"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:layout_columnWeight="2">
            <TextView
                android:id="@+id/center_name45"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="NA"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15dp" />
        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_gravity="center_vertical"
            android:layout_height="50dp"
            android:background="@color/black"/>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_gravity="center_vertical"
            android:layout_columnWeight="1">
            <TextView
                android:id="@+id/age_Textview45"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="45+"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15dp" />
        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_gravity="center_vertical"
            android:layout_height="50dp"
            android:background="@color/black"/>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_gravity="center_vertical"
            android:layout_columnWeight="1">
            <TextView
                android:id="@+id/dose1_Textview45"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NA"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15dp" />
        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_gravity="center_vertical"
            android:layout_height="50dp"
            android:background="@color/black"/>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_gravity="center_vertical"
            android:layout_columnWeight="1">
            <TextView
                android:id="@+id/dose2_Textview45"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NA"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15dp" />
        </LinearLayout>


    </GridLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/vaccine_name_txtview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="VACCINE NAME :- NA"
            android:textColor="@color/black"
            android:paddingLeft="20dp"/>

        <TextView
            android:id="@+id/price_txtview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="VACCINE PRICE :- NA"
            android:textColor="@color/black"
            android:paddingLeft="20dp"/>


        <TextView
            android:id="@+id/address_txtview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ADDRESS :- NA"
            android:textColor="@color/black"
            android:paddingLeft="20dp"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_height="2dp"
        android:layout_marginVertical="5dp"

        android:background="@color/black"/>


</LinearLayout>